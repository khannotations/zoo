!!! 5
%head
  %title Zoo | Yale University
  %meta{:charset => "utf-8"}
  %meta{"http-equiv" =>"X-UA-Compatible", :content => "IE=edge,chrome=1"}
  %link{:href => "http://netdna.bootstrapcdn.com/twitter-bootstrap/2.2.1/css/bootstrap-combined.min.css", :rel => "stylesheet"}/
  %link{:rel => "stylesheet", :href => "/styles"}/
  %link{:rel=>"shortcut icon", :href=>"/images/zoo.png"}

%body
  :plain
    <script type="text/javascript">
      var _gaq = _gaq || [];
      _gaq.push(['_setAccount', 'UA-39084854-1']);
      _gaq.push(['_trackPageview']);
      (function() {
        var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
        ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
        var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
      })();
    </script>
  = haml :backs
  #zoo-nav.zoo-bar
    .container
      %table.zoo-navs
        = haml :nav
  = yield

  %script{:src => "http://ajax.googleapis.com/ajax/libs/jquery/1.8.1/jquery.min.js", :type => "text/javascript"}
  %script{:src => "http://netdna.bootstrapcdn.com/twitter-bootstrap/2.2.1/js/bootstrap.min.js"}
  %script{:src => "/javascripts/moment.min.js"}
  %script{:src => "/scripts", :type => "text/javascript"}